<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sakis.anthologium.main.MainController">
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
   <children>
      <GridPane prefHeight="120.0" prefWidth="130.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="actorButton" mnemonicParsing="false" onAction="#handleActorButton" prefHeight="27.0" prefWidth="166.0" text="Actors" />
            <Button fx:id="songButton" mnemonicParsing="false" onAction="#handleSongButton" prefHeight="27.0" prefWidth="170.0" text="Songs" GridPane.rowIndex="2" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#handleExitButton" prefHeight="27.0" prefWidth="145.0" text="Exit" GridPane.rowIndex="3" />
            <Button fx:id="photoButton" mnemonicParsing="false" onAction="#handlePhotoButton" prefHeight="27.0" prefWidth="137.0" text="Photos" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
